include partials/head.jade
include partials/header.jade
include welcome.jade
include partials/footer.jade
include partials/stub.jade


doctype html5
html
  head
    +head()
  body
    div.page-wrapper
      +header(loggedIn)
      section
        +welcome()



      section.projectList
        div.pure-g
          div.pure-u-1.pure-u-1-4
            h2 CODE PROJECTS
            i Join a project you're interested in!
          div.pure-u-1.pure-u-3-4


        div.projects(id="project-list")
          - for(var i=0; i<projects.length; i++){
            - if(projects[i].title)
              +stub(projects[i])
          - }


    +footer()

